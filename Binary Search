#include<stdio.h>
int main()
{
	int A[20],n,key,flag=0,pos,i,F,L,mid;
	printf("Enter the no of elements:");
	scanf("%d",&n);
	printf("enter the array elements:");
	for(i=0;i<n;i++)
	scanf("%d",&A[i]);
	printf("Enter the key:");
	scanf("%d",&key);
	F=0;
	L=n-1;
	for(i=0;i<n;i++)
	{
		mid=(F+L)/2;
		if(A[mid]==key)
		{
			flag=1;
			pos=mid;
			break;
		}
		else if(A[mid]>key)
		L=mid-1;
		else if(A[mid]<key)
		F=mid+1;
	}
	if(flag==1)
	printf("The element %d is at position %d",key,pos+1);
	else
	printf("The element is not in the list");
	return 0;
}
		
	
	
